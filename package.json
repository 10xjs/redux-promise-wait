{
  "name": "redux-promise-wait",
  "version": "0.1.0",
  "author": "Neal Granger",
  "license": "CC0-1.0",
  "repository": "nealgranger/redux-promise-wait",
  "scripts": {
    "build": "babel -s inline -d . src/",
    "lint": "eslint .",
    "lint:dev": "eslint . || true",
    "prepublish": "npm run build",
    "dev": "watch 'npm run test:dev' src/ test/ & npm run build -- -w",
    "spec": "NODE_ENV=test mocha --compilers js:babel-core/register -r adana-dump -R spec test/spec",
    "spec:dev": "NODE_ENV=test mocha --compilers js:babel-core/register test/spec || true",
    "test": "npm run lint && npm run spec",
    "test:dev": "npm run lint:dev && npm run spec:dev"
  },
  "dependencies": {
    "is-promise": "^2.1.0"
  },
  "peerDependencies": {
    "redux": ">=2.0.0"
  },
  "devDependencies": {
    "adana-cli": "^0.1.0",
    "adana-dump": "^0.1.0",
    "adana-format-lcov": "^0.1.1",
    "babel-cli": "^6.4.0",
    "babel-core": "^6.3.26",
    "babel-preset-metalab": "^0.1.4",
    "chai": "^3.4.0",
    "eslint": "^1.10.3",
    "eslint-config-metalab": "^1.0.0-rc.4",
    "eslint-plugin-filenames": "^0.2.0",
    "eslint-plugin-import": "^0.11.0",
    "eslint-plugin-react": "^3.15.0",
    "mocha": "^2.3.3",
    "redux": "^3.0.5",
    "sinon": "^1.17.2",
    "sinon-chai": "^2.8.0",
    "watch": "^0.17.1"
  },
  "browser": "browser.js"
}
